    "@types/express": "^4.17.15",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^14.18.2",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.11",
    "dotenv-cli": "^5.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.20",
    "prisma": "^3.15.2",
    "supertest": "^6.3.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^2.2.3"
  },
